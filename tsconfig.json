{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@lib/*": ["src/lib/*"],
			"@types/*": ["src/types/*"],
			"@images/*": ["src/images/*"],
			"@logos/*": ["src/logos/*"],
			"@utils/*": ["src/utils/*"],
			"@services/*": ["src/services/*"],
		}
	}
}
